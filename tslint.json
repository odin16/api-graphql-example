{
  "extends": ["tslint-config-airbnb"],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "import-name": false,
    "max-line-length": [true, 110],
    "variable-name": false
  }
}